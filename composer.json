{
    "name": "chaplean/social-buttons-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Template for social buttons",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://www.chaplean.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\SocialButtonsBundle\\": ""
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "chaplean/coding-standard": "1.1.*",
        "chaplean/unit-bundle": "~3.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true
    }
}
