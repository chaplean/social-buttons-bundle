{
    "name": "chaplean/social-buttons-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Template for social buttons",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\SocialButtonsBundle\\": "",
            "Tests\\": "Tests"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.coop"
        }
    ],
    "require": {
        "php": ">=7.1",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "sensio/distribution-bundle": "^5.0.19",
        "symfony/symfony": "^3.4",
        "symfony/assetic-bundle": "^2.8"
    },
    "require-dev": {
        "chaplean/coding-standard": "1.1.*",
        "chaplean/unit-bundle": "^7.0",
        "mockery/mockery": "dev-master",
        "symfony/phpunit-bridge": "^4.0"
    },
    "config": {
        "bin-dir": "bin"
    }
}
